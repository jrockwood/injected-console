{
  "name": "@jrockwood/injected-console-monorepo",
  "version": "2.0.0",
  "description": "Monorepo for the injected-console projects",
  "repository": "https://github.com/jrockwood/injected-console.git",
  "author": "Justin Rockwood",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/jasmine",
      "**/jasmine/**",
      "**/jasmine-expect",
      "**/jasmine-expect/**"
    ]
  },
  "scripts": {
    "build": "yarn install && yarn run clean && yarn run compile && yarn run test",
    "clean": "yarn workspaces run clean",
    "compile": "yarn workspaces run compile",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "3.10.1",
    "@types/node": "16.11.4",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "nodemon": "2.0.14",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "fs-extra": "10.0.0"
  }
}
